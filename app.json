{
  "expo": {
    "name": "Social Uptime",
    "slug": "uptime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "socialuptime",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0A14"
    },
    "ios": {
      "bundleIdentifier": "com.rrtechsolutions.social-uptime",
      "supportsTablet": true,
      "icon": "./assets/icon.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": ["applinks:ridhwan.io"],
      "appleTeamId": "89X78S524T"
    },
    "android": {
      "package": "com.rrtechsolutions.social-uptime",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#FF0050",
          "sounds": []
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "aaccd917-fadf-40b5-b66f-bd85ac8a9e0d"
      }
    },
    "owner": "ridhwanromjon"
  }
}
